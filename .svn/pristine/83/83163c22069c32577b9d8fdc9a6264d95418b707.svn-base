package com.kjlink.privilege.bean;

/**
 * 用户实体类
 * @author liuxy
 *
 */
public class User {

	private Integer id;
	
	private String username;
	
	private String password;
	
	private String gander;
	
	private String birthPlace;
	
	private String[] interest;
	
 
	public User(Integer id, String username, String gander, String birthPlace, String[] interest) {
		 this.id=id;
		 this.username=username;
		 this.gander=gander;
		 this.birthPlace=birthPlace;
		 this.interest=interest;
	}

	public User() { 
	}
 
	
	public User(String username, String password, String gander, String birthPlace, String[] interest) {
		super();
		this.username = username;
		this.password = password;
		this.gander = gander;
		this.birthPlace = birthPlace;
		this.interest = interest;
	}

 
	public User(int id2, String username2, String gander2, String birthPlace2, String hobby) {
		// TODO Auto-generated constructor stub
	}

	@Override
	public String toString() {
		return "User [username=" + username + ", password=" + password + ", gander=" + gander 
				+ ", birthPlace=" + birthPlace + ", interest=" + interest + "]";
	}
	
	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getGander() {
		return gander;
	}

	public void setGander(String gander) {
		this.gander = gander;
	}

	public String getBirthPlace() {
		return birthPlace;
	}

	public void setBirthPlace(String birthPlace) {
		this.birthPlace = birthPlace;
	}

	public String[] getInterest() {
		return interest;
	}

	public void setInterest(String[] interest) {
		this.interest = interest;
	}

}
