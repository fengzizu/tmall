package com.kjlink.privilege.bean;

/**
 * 用户-角色关系实体类
 * @author liuxy
 *
 */
public class UserRole{
	
	private Integer id;
	
	private User user;
	
	private Role role;
	
	public UserRole(Integer id) {
		
		this.id = id;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public Role getRole() {
		return role;
	}

	public void setRole(Role role) {
		this.role = role;
	}
	
}
